<Window x:Class="OperationLog.Presentation.Desktop.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:viewModel="clr-namespace:OperationLog.Presentation.Desktop.ViewModel"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Window.Resources>
        <viewModel:BaseViewModel x:Key="BaseViewModel" />
    </Window.Resources>
    <Grid DataContext="{StaticResource BaseViewModel}">
        <ListBox ItemsSource="{Binding Users}"></ListBox>
    </Grid>
</Window>
